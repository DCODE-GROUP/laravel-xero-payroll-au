{
  "name": "dcodegroup/laravel-xero-payroll-au",
  "description": "Simple package which dcode uses to handle the syncing of payroll related information from Xero",
  "keywords": [
    "laravel",
    "xero",
    "dcode",
    "payroll-au",
    "payroll"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Nathan Rzepecki",
      "email": "nathan@lionslair.net.au",
      "homepage": "https://www.lionslair.net.au",
      "role": "Developer"
    },
    {
      "name": "Dcode Group",
      "email": "forge@dcodegroup.com",
      "homepage": "https://dcodegroup.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "dcodegroup/laravel-xero-oauth": "^1.0",
    "dcodegroup/laravel-configuration": "^0.1",
    "laravel/framework": "^7.0  | ^8.0"
  },
  "autoload": {
    "psr-4": {
      "Dcodegroup\\LaravelXeroPayrollAu\\": "src"
    }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dcodegroup\\LaravelXeroPayrollAu\\LaravelXeroPayrollAuServiceProvider"
      ]
    }
  },
  "require-dev": {
    "laravel/pint": "^1.2"
  }
}
